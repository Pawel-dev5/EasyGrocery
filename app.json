{
	"expo": {
		"name": "Easy\nGrocery",
		"slug": "easy-grocery",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/EasyGrocery.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.EasyGrocery.ios.app"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png"
			},
			"package": "com.EasyGrocery.android.app"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-community-flipper"],
		"extra": {
			"eas": {
				"projectId": "7a186aca-db6b-49e4-a969-1ca1b02e9d1a"
			}
		}
	}
}
